{
  "name": "react-app-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc && eslint . --ext .ts,.tsx && stylelint **/*.scss",
    "lint:fix": "tsc && eslint . --ext .ts,.tsx --fix && stylelint **/*.scss --fix",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --passWithNoTests --ui",
    "test:coverage": "vitest --coverage --watch false"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
